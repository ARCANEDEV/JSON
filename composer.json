{
    "name":        "arcanedev/json",
    "description": "JSON Helper for PHP projects",
    "keywords":    ["arcanedev", "json", "encode", "decode"],
    "homepage":    "https://github.com/ARCANEDEV/JSON",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                   ">=5.6.4",
        "ext-json":              "*",
        "illuminate/contracts":  "~5.0",
        "illuminate/filesystem": "~5.0",
        "illuminate/support":    "~5.0"
    },
    "require-dev": {
        "phpunit/phpcov":         "~3.0",
        "phpunit/phpunit":        "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Json\\": "src/"
        },
        "files": ["constants.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Json\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
